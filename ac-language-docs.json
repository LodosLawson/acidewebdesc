  "examples": {
    "helloWorld": {
      "title": "Merhaba Dünya",
      "code": "baslanqic\n    yazdir: \"Merhaba Dünya\"\nson",
      "output": "Merhaba Dünya"
    },
    "variables": {
      "title": "Değişkenler",
      "code": "baslanqic\n    reqem sayi = 10\n    herif mesaj = \"AC Dili\"\n    yazdir: mesaj, \":\", sayi\nson",
      "output": "AC Dili : 10"
    },
    "functions": {
      "title": "Fonksiyonlar",
      "code": "baslanqic\n    fonksiyon topla(reqem a, reqem b) : reqem {\n        geri a + b\n    }\n    \n    reqem sonuc = topla<5, 3>\n    yazdir: \"Toplam:\", sonuc\nson",
      "output": "Toplam: 8"
    },
    "conditionals": {
      "title": "Koşullu İfadeler",
      "code": "baslanqic\n    reqem sayi = 5\n    \n    dif (sayi > 0) {\n        yazdir: \"Pozitif\"\n    } eksedif (sayi < 0) {\n        yazdir: \"Negatif\"\n    } eks {\n        yazdir: \"Sıfır\"\n    }\nson",
      "output": "Pozitif"
    },
    "factorial": {
      "title": "Faktöriyel Hesaplama",
      "code": "baslanqic\n    fonksiyon faktoriyel(reqem n) : reqem {\n        dif (n <= 1) {\n            geri 1\n        }\n        geri n * faktoriyel<n - 1>\n    }\n    \n    reqem sayi = 5\n    yazdir: \"Faktöriyel:\", faktoriyel<sayi>\nson",
      "output": "Faktöriyel: 120"
    },
    "calculator": {
      "title": "Hesap Makinesi",
      "code": "baslanqic\n    fonksiyon hesapla(reqem a, reqem b, herif islem) : reqem {\n        dif (islem == \"+\") { geri a + b }\n        dif (islem == \"-\") { geri a - b }\n        dif (islem == \"*\") { geri a * b }\n        dif (islem == \"/\") { geri a / b }\n        geri 0\n    }\n    \n    reqem sayi1 = 10\n    reqem sayi2 = 5\n    \n    yazdir: \"Toplama:\", hesapla<sayi1, sayi2, \"+\">\n    yazdir: \"Çıkarma:\", hesapla<sayi1, sayi2, \"-\">\nson",
      "output": "Toplama: 15\nÇıkarma: 5"
    }
  },
