  "syntax": {
    "programStructure": {
      "description": "Her AC programı baslanqic ile başlamalı ve son ile bitmelidir",
      "required": true,
      "example": "baslanqic\n    // kod buraya\nson"
    },
    "variableDeclaration": {
      "syntax": "tip isim = deger",
      "examples": [
        "reqem sayi = 5",
        "herif mesaj = \"Merhaba\"",
        "ustglobal reqem globalSayi = 10"
      ],
      "rules": [
        "Değişken isimleri harf, rakam ve alt çizgi içerebilir",
        "Değişken isimleri rakamla başlayamaz",
        "Değişken isimleri büyük/küçük harf duyarlıdır"
      ]
    },
    "functionDeclaration": {
      "syntax": "fonksiyon isim(parametreler) : donusTipi { kod }",
      "examples": [
        "fonksiyon topla(reqem a, reqem b) : reqem {\n    geri a + b\n}",
        "fonksiyon yazdirMesaj(herif mesaj) {\n    yazdir: mesaj\n}"
      ],
      "rules": [
        "Fonksiyon parametreleri parantez içinde tanımlanır",
        "Dönüş tipi iki nokta (:) ile belirtilir",
        "Fonksiyon gövdesi süslü parantez içinde olmalıdır",
        "Değer döndürmek için 'geri' anahtar kelimesi kullanılır"
      ],
      "alternativeSyntax": {
        "description": "Eski versiyonlarda DIF: syntax'ı kullanılırdı",
        "example": "DIF: fonksiyon_ismi<parametreler>{ kod }",
        "legacy": true
      }
    },
    "functionCall": {
      "syntax": "isim<parametreler>",
      "examples": [
        "topla<5, 3>",
        "yazdirMesaj<\"Merhaba\">"
      ],
      "rules": [
        "Fonksiyon çağrılarında < ve > karakterleri kullanılır",
        "Parametreler virgülle ayrılır",
        "Parametreler opsiyoneldir"
      ]
    },
    "printStatement": {
      "syntax": "yazdir: deger1, deger2, ...",
      "examples": [
        "yazdir: \"Merhaba Dünya\"",
        "yazdir: sayi",
        "yazdir: \"Sonuç:\", sonuc",
        "yazdir: \"Toplam:\", topla<5, 3>"
      ],
      "rules": [
        "İki nokta (:) zorunludur",
        "Birden fazla değer virgülle ayrılabilir",
        "String değerler çift tırnak içinde olmalıdır"
      ]
    },
    "conditionalStatement": {
      "syntax": "dif (kosul) { kod } eksedif (kosul) { kod } eks { kod }",
      "examples": [
        "dif (sayi > 0) {\n    yazdir: \"Pozitif\"\n}",
        "dif (sayi > 0) {\n    yazdir: \"Pozitif\"\n} eksedif (sayi < 0) {\n    yazdir: \"Negatif\"\n} eks {\n    yazdir: \"Sıfır\"\n}"
      ],
      "rules": [
        "Koşul parantez içinde olmalıdır",
        "Kod bloğu süslü parantez içinde olmalıdır",
        "eksedif ve eks opsiyoneldir"
      ]
    },
    "comments": {
      "singleLine": {
        "syntax": "// yorum",
        "example": "// Bu bir yorum satırı"
      },
      "multiLine": {
        "syntax": "/* yorum */",
        "example": "/* Bu çok satırlı\nyorum bloğudur */"
      }
    },
    "stringLiterals": {
      "syntax": "\"metin\"",
      "examples": [
        "\"Merhaba Dünya\"",
        "\"Sayı: \" + sayi"
      ],
      "rules": [
        "String değerler çift tırnak içinde olmalıdır",
        "String içinde özel karakterler escape edilebilir"
      ]
    },
    "numberLiterals": {
      "types": ["integer", "float"],
      "examples": ["5", "10", "3.14", "2.5"],
      "rules": [
        "Tam sayılar doğrudan yazılabilir",
        "Ondalıklı sayılar nokta ile ayrılır"
      ]
    }
